{
  "name": "@jymfony/validator",
  "version": "0.1.0-alpha.30",
  "description": "Jymfony Validator component",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "node ./tests.js"
  },
  "keywords": [
    "validation",
    "validator",
    "bean validator",
    "jsr-303",
    "jymfony"
  ],
  "author": "Alessandro Chitolina <alekitto@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@jymfony/autoloader": "0.1.0-alpha.30",
    "@jymfony/datetime": "0.1.0-alpha.30",
    "@jymfony/exceptions": "0.1.0-alpha.30",
    "@jymfony/metadata": "0.1.0-alpha.30",
    "@jymfony/util": "0.1.0-alpha.30"
  },
  "devDependencies": {
    "@jymfony/debug": "*",
    "@jymfony/dependency-injection": "*t",
    "@jymfony/testing": "*",
    "@jymfony/var-dumper": "*",
    "@jymfony/yaml": "*",
    "chai": "^4.2",
    "mocha": "^10.2"
  },
  "config": {
    "jymfony-autoload": {
      "includes": [
        "src/Test/ChaiExpectation.js"
      ],
      "namespaces": {
        "Jymfony.Component.Validator": "src/"
      }
    },
    "jymfony-autoload-dev": {
      "namespaces": {
        "Jymfony.Component.Validator.Fixtures": "fixtures/",
        "Jymfony.Component.Validator.Tests": "test/"
      }
    }
  }
}
