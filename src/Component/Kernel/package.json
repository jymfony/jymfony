    "@jymfony/dependency-injection": "0.1.0-alpha.30",
    "@jymfony/exceptions": "0.1.0-alpha.30",
    "@jymfony/util": "0.1.0-alpha.30"
  },
  "devDependencies": {
    "@jymfony/debug": "*",
    "@jymfony/filesystem": "*",
    "@jymfony/testing": "*",
    "chai": "^4.2",
    "mocha": "^10.2"
  },
  "config": {
