{
  "name": "@jymfony/http-client",
  "version": "0.1.0-dev",
  "description": "Jymfony HttpClient component",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "node ./tests.js"
  },
  "keywords": [
    "lexer",
    "jymfony"
