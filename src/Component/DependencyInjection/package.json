{
  "name": "@jymfony/dependency-injection",
  "version": "0.1.0-alpha.25",
  "description": "Jymfony Dependency Injection",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "node ./tests.js test/**"
  },
  "keywords": [
    "di",
    "dependency injection",
    "jymfony"
  ],
  "author": "Alessandro Chitolina <alekitto@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "@jymfony/autoloader": "0.1.0-alpha.25",
    "@jymfony/config": "0.1.0-alpha.25",
    "@jymfony/datastructure": "0.1.0-alpha.25",
    "@jymfony/decorators": "0.1.0-alpha.25",
    "@jymfony/exceptions": "0.1.0-alpha.25",
    "@jymfony/property-access": "0.1.0-alpha.25",
    "@jymfony/util": "0.1.0-alpha.25"
  },
  "devDependencies": {
    "@jymfony/debug": "*",
    "@jymfony/testing": "*",
    "@jymfony/var-dumper": "*",
    "@jymfony/yaml": "*",
    "chai": "^4.2",
    "mocha": "^6.0"
  },
  "config": {
    "jymfony-autoload": {
      "namespaces": {
        "Jymfony.Component.DependencyInjection": "src/"
      }
    },
    "jymfony-autoload-dev": {
      "namespaces": {
        "Jymfony.Component.DependencyInjection.Fixtures": "fixtures/",
        "Jymfony.Component.DependencyInjection.Tests": "test/"
      }
    }
  }
}
