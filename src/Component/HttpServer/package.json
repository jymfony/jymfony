{
  "name": "@jymfony/http-server",
  "version": "0.1.0-alpha.30",
  "description": "Jymfony Http Server",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "node ./tests.js"
  },
  "keywords": [
    "application",
